    "react-dom": "^18",
    "react-player": "^2.14.1",
    "react-syntax-highlighter": "^15.5.0",
    "sharp": "^0.32.6",
    "ua-parser-js": "^2.0.0-beta.1",
    "usehooks-ts": "^2.9.4",
    "zod": "^3.22.4"
