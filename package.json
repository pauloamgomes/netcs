    "eslint-config-next": "^14.0.4",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^4.5.1"
